.eye-home-contain,
.home-contain-slider {
  width: 100%;
  height: 200px;
}

.home-contain-slider {
  position: relative;
}

.home-contain-more {
  width: 100%;
  height: 40px;
  background: #fff;
  text-align: center;
  border-bottom: 1px solid #ccc;
}

.home-contain-more text {
  font-size: 12px;
  line-height: 40px;
  color: #333;
}

.contain-detail-show {
  width: 100%;
  height: 250px;
  margin-bottom: 10px;
}
.detail-show-img {
  width: 100%;
  height: 200px;
}
.contain-detail-intro {
  width: 100%;
  height: 50px;
}
.contain-detail-img {
  width: 20%;
  text-align: center;
  margin-top: 8px;
  float: left;
}
.contain-detail-head {
  width: 30px;
  height: 30px;
  border: 1px solid #ccc;
  border-radius: 16px;

}
.contain-detail-main {
  width: 80%;
  float: left;
}
.detail-main-title,
.detail-main-introduce {
  display: block;
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.detail-main-title {
  height: 28px;
  font-size: 14px;
  line-height: 28px;
  color: #000;
  font-weight: 600;
  margin-top: 5px;
}
.detail-main-introduce {
  font-size: 12px;
  line-height: 12px;
  color: #ccc;
}

.home-contain-load-more {
  width: 100%;
  height: 50px;
  font-size: 16px;
  color: #ccc;
  text-align: center;
  line-height: 50px;
  border-top: 1px solid #ccc;
}