  width: 100%;
  color: #333;
}

.eye-mine-head {
  width: 100%;
  height: 200px;
  text-align: center;
  border-bottom: 1px solid #ccc;
}
.mine-head-icon {
  width: 50px;
  height: 50px;
  border: 1px solid #ccc;
  border-radius: 26px;
  margin-top: 20px;

}
.mine-head-name,
.mine-head-homepage {
  display: block;
}
.mine-head-name {
  font-size: 18px;
  line-height: 20px;
}
.mine-head-homepage {
  font-size: 12px;
  color: #ccc;
  line-height: 18px;
  margin-top: 5px;
}
.mine-head-choose {
  width: 100%;
}
.head-choose-collect,
.head-choose-comment {
  width: 50%;
  font-size: 14px;
  color: #ccc;
  margin-top: 25px;
  float: left;
}
